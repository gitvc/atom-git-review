{
  "name": "git-review",
  "main": "./lib/git-review",
  "version": "0.3.0",
  "description": "Use git-review in atom.",
  "keywords": [
    "gerrit",
    "git",
    "review"
  ],
  "activationCommands": {
    "atom-workspace": [
       "git-review:version",
       "git-review:menu",
       "git-review:submit",
       "git-review:list",
       "git-review:download"
    ]
  },
  "repository": "https://github.com/wenlock/atom-git-review",
  "license": "Apache2",
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "fs-plus": "^2.2.0",
    "fuzzaldrin": "^1.2.0",
    "underscore-plus": "1.x",
    "atom-space-pen-views": "^2.0.3"
  }
}
